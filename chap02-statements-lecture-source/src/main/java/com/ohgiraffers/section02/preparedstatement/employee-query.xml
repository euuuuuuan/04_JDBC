<?xml version="1.0" encoding="utf-8" ?> <!--선언부 반드시 처음에 나와야함-->
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd"> <!--문서의 형식이고 문서의 유효성을 검사하기 위한 것-->
<properties>
    <comment/>
    <!--key가 들어가는 위치-->
        <entry key="selectEmpByFamilyName">
    <!--value가 들어가는 위치-->
            select
            *
            from
            employee
            where
            emp_name like concat(?, '%')
        </entry>
</properties>